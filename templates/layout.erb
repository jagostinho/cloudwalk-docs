<body>
  <div class="navbar navbar-inverse navbar-fixed-top">
    <div class="navbar-inner">
      <div class="navbar-container">
        <button type="button" class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse">
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
        </button>
        <div class="nav-collapse collapse">
          <ul class="nav">
            <li><a href="https://www.cloudwalk.io"><%= I18n.t("main_menu.home") %></a></li>
            <li><a href="https://manager.cloudwalk.io">CloudWalk Manager</a></li>
            <li class="active"><a href="https://docs.cloudwalk.io/<%= I18n.locale %>/introduction"><%= I18n.t("main_menu.docs") %></a></li>
            <li><a href="https://support.cloudwalk.io"><%= I18n.t("main_menu.support") %></a></li>
            <li><a href="mailto:planobe@planobe.com.br"><%= I18n.t("main_menu.get_in_touch") %></a></li>
            <li style="float: right">
              <form action="<%= request.fullpath %>" method="get" class="navbar-form pull-right" style="float: right">
                <% if params[:query] %>
                  <input type="hidden" name="query" value="<%= params['query']%>" />
                <% end %>
                <select name="locale" class="input-medium" onchange="this.form.submit()">
                  <%= option_select "pt-BR", "Português" %>
                  <%= option_select "en", "English" %>
                </select>
              </form>
            </li>
            <li id="btn-signout" class="btn-account" style="float: right"><a href="https://manager.cloudwalk.io/users/sign_out?locale=<%= I18n.locale %>"><%= I18n.t("main_menu.sign_out") %></a></li>
            <li id="btn-settings" class="btn-account" style="float: right"><a href="https://manager.cloudwalk.io/users/edit?locale=<%= I18n.locale %>"><%= I18n.t("main_menu.settings") %></a></li>
            <li id="btn-signup" class="btn-account" style="float: right"><a href="https://manager.cloudwalk.io/users/sign_up?locale=<%= I18n.locale %>"><%= I18n.t("main_menu.sign_up") %></a></li>
            <li id="btn-signin" class="btn-account" style="float: right"><a href="https://manager.cloudwalk.io/users/sign_in?locale=<%= I18n.locale %>"><%= I18n.t("main_menu.sign_in") %></a></li>
          </ul>
        </div>
      </div>
    </div>
  </div>

  <div class="container-fluid" id="top">
    <div class="row-fluid">
      <div class="span3">
        <%= erb :_menu %>
      </div>
      <div class="span9">
        <div id="banner">
          <form action="/<%= current_locale %>/search" class="navbar-form pull-right" method="GET" id="indextank_autocomplete">
            <input name="query" id="query" class="input-medium search-query" placeholder="<%= I18n.t("general.search") %>" type="text" value="<%= params[:query]%>" />
          </form>
        </div>
        <%= yield %>
      </div>
    </div>
  </div>

