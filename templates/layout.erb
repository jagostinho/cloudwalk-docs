  <script src="/js/bootstrap.min.js"></script>
  <script src="/js/jquery-ui.min.js"></script>
  <script src="/js/jquery.indextank.ize.js"></script>
  <script src="/js/jquery.indextank.autocomplete.js"></script>
  <script src="/js/rainbow/rainbow-custom.min.js"></script>
  <script src="/js/jquery.cookie.js"></script>

  <% if session[:locale] == "pt-BR" %>
    <script src="//cdn.moot.it/1/moot.pt-br.min.js"></script>
  <% else %>
    <script src="//cdn.moot.it/1/moot.min.js"></script>
  <% end %>

  <script type="text/javascript">
    jQuery(document).ready(function(){
      $('a[href*=#]').not('.accordion a, .nav-tabs a').click(function() {if (location.pathname.replace(/^\//,'') == this.pathname.replace(/^\//,'') && location.hostname == this.hostname) {var $target = $(this.hash);$target = $target.length && $target || $('[name=' + this.hash.slice(1) +']');if ($target.length) {var targetOffset = $target.offset().top - 50;$('html,body').animate({scrollTop: targetOffset}, 500);return false;}}});

      // commands
      $(".group-of-commands a").each(function() { $(this).click(function() { showContent($(this).attr('id')); }); });
      function showContent(caller){
        var container = caller.split('-')[1];
        $('.posxml-commands').fadeOut("fast");
        if(container == 'all'){ $('.posxml-commands').fadeIn("slow"); } else { $('#' + container + '-commands').fadeIn("slow"); }
        $('.group-of-commands a').removeClass('primary');
        $('#' + caller).addClass('primary');
      }

      // set menu
      if($.cookie('is_user_logged_in')){
        $("#btn-signout").toggle();
        $("#btn-settings").toggle();
      } else {
        $("#btn-signup").toggle();
        $("#btn-signin").toggle();
      }

      // IndexTank
      var publicApiUrl = "https://28z3j.api.searchify.com";
      var indexName = "cloudwalk-docs";
      $("#indextank_autocomplete").indextank_Ize(publicApiUrl, indexName);
      $("#query").indextank_Autocomplete();

      // Rainbow
      Rainbow.extend('html', [{
        'name': 'tag',
        'pattern': /string.tohex|string.replace|string.substring|string.replaceat|string.removeat|string.insertat|string.find|string.elementat|string.elements|string.charat|string.length|string.pad|string.getvaluebykey|string.fromhex|network.send|network.receive|network.ping|network.checkgprssignal|network.hostdisconnect|system.readcard|system.restart|system.info|system.checkbattery|system.beep|file.open|file.read|file.write|file.close|filesystem.filesize|filesystem.space|crypto.xor|crypto.lrc|crypto.encryptdecrypt|crypto.crc|smartcard.insertedcard|input.getvalue|iso8583.initfieldtable|iso8583.initmessage|iso8583.putfield|iso8583.endmessage|iso8583.transactmessage|iso8583.analyzemessage|iso8583.getfield|time.calculate/g
      },  {
        'name': 'support',
        'pattern': /buffer1|buffer2|element_index|new_substring|old_substring|original_string|new_element|string_to_be_inserted|character_index/g
      }], true);

      <%= yield_content(:javascripts) %>
    })
  </script>
</body>
</html>