  <li><%= I18n.t("posxml.commands.checkpaperout.parameters.parameter_1") %></li>
</ol>

<h2><%= I18n.t("posxml.commands.example") %></h2>

<pre><code data-language="html">&lt;integervariable value="0" variable="iPaper"/&gt;

&lt;checkpaperout variablereturn="$(iPaper)="/&gt;

&lt;if variable="$(iPaper)" operator="equalto" value="0"&gt;
  &lt;display column="0" line="0" message="Printer without paper"/&gt;
&lt;else/&gt;
  &lt;display column="0" line="0" message="Printer with papger"/&gt;
&lt;/if&gt;

&lt;waitkey/&gt;
</code></pre>

