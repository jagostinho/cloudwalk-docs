<h1><%= I18n.t("posxml.commands.title") %> <em>file.close</em></h1>

<p class="more-info"><%= I18n.t("general.updated_at", :date => "03/05/2013") %></p>

<p><%= I18n.t("posxml.commands.file_close.paragraph_1") %></p>

<h2><%= I18n.t("posxml.commands.parameters") %></h2>

<ol class="parameters">
  <li><%= I18n.t("posxml.commands.file_close.parameters.parameter_1") %></li>
</ol>

<h2><%= I18n.t("posxml.commands.example") %></h2>

<pre><code data-language="html">&lt;integervariable value="0" variable="iHandle" /&gt;
&lt;integervariable value="0" variable="iReturn" /&gt;

&lt;downloadfile filename="test.txt" remotepath="test.txt" variablereturn="$(iReturn)" /&gt;

&lt;file.open filename="test.txt" mode="r" variablehandle="$(iHandle)"  /&gt;

&lt;display column="0" line="0" message="File is open" /&gt;

&lt;waitkey /&gt;

&lt;file.close handle="$(iHandle)" /&gt;

&lt;display column="0" line="0" message="File is closed" /&gt;

&lt;waitkey /&gt;
</code></pre>
