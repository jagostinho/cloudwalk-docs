  <li><%= I18n.t("posxml.commands.openserialport.parameters.parameter_1") %></li>
  <li><%= I18n.t("posxml.commands.openserialport.parameters.parameter_2") %></li>
  <li><%= I18n.t("posxml.commands.openserialport.parameters.parameter_3") %></li>
